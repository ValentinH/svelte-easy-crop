{
  "name": "svelte-easy-crop",
  "version": "2.0.0",
  "svelte": "src/index.svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "description": "A Svelte component to crop images with easy interactions",
  "homepage": "https://github.com/ValentinH/svelte-easy-crop",
  "repository": {
    "type": "git",
    "url": "https://github.com/ValentinH/svelte-easy-crop"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "Valentin Hervieu <valentin@hervi.eu>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p start:dev build:dev",
    "check": "svelte-check",
    "build:dev": "rollup -c rollup.config.example.js -w",
    "start:dev": "sirv example/public --dev -p 3000",
    "prepublishOnly": "npm run build",
    "test": "jest && yarn e2e",
    "unit": "jest",
    "unit:watch": "jest --watchAll",
    "e2e": "start-server-and-test dev http://localhost:3000 cy:run",
    "e2e:ci": "start-server-and-test dev http://localhost:3000 cy:ci",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "cypress run --record",
    "npm:publish": "np"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "cypress": "7.6.0",
    "jest": "^27.0.5",
    "np": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "query-string": "^6.5.0",
    "rollup": "^2.38.3",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.3.1",
    "sirv-cli": "^2.0.2",
    "start-server-and-test": "^1.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "svelte",
    "image crop",
    "cropper"
  ],
  "files": [
    "src",
    "dist"
  ]
}
